{
  "expo": {
    "name": "Lesson Vault",
    "slug": "Lesson-Vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lessonsmanager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.arezkicherfouh.LessonVault"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1x1-px.png",
        "backgroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.arezkicherfouh.LessonVault",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F5F4F0",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#111110"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow LessonVault to take photos."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow LessonVault to access your photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "828d8d30-245f-4b61-8279-0a1020b0abac"
      }
    },
    "owner": "arezki-cherfouh"
  }
}
